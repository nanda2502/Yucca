{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "GDB Launch",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/build/yucca",
        "args": ["0", "25"],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}/build",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "miDebuggerPath": "/usr/bin/gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ],
        "internalConsoleOptions": "openOnSessionStart",
        "logging": {
          "moduleLoad": false,
          "trace": true
        }
      }
    ]
  }